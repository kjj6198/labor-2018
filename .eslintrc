{
  "plugins": ["smells", "react"],
  "extends": "airbnb",
  "globals": {
    "$": true,
    "window": true,
    "document": true
  },
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "no-debugger": "off",
    "react/jsx-filename-extension": 0,
    "react/require-default-props": "off",
    "default-case": 0,
    "no-multi-spaces": 0
  }
}
